<!DOCTYPE html>
<html lang="en">

<head>
    <%- include ('../components/head') %>
</head>

<body class="container">

    <header>
        <%- include ('../components/header') %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>User List</h1>
            <ul>
                <% characters.forEach(function(character) { %>
                <li><a href="/character/<%= character.id %>"><%= character.name %></a></li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include ('../components/footer') %>
    </footer>

</body>

</html>